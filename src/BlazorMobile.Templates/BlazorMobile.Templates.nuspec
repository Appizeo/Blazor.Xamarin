<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
	<id>BlazorMobile.Templates</id>
	<version>$version$</version>
	<title>BlazorMobile.Templates</title>
	<authors>Guillaume ZAHRA</authors>
	<owners>Daddoon</owners>
	<licenseUrl>https://github.com/Daddoon/BlazorMobile/blob/master/LICENSE</licenseUrl>
	<projectUrl>https://github.com/Daddoon/BlazorMobile</projectUrl>
	<iconUrl>https://raw.githubusercontent.com/Daddoon/BlazorMobile/master/logo_blazormobile_256x256.png</iconUrl>
	<requireLicenseAcceptance>false</requireLicenseAcceptance>
	<description>A project template for creating an iOS, Android &amp; UWP app from a Blazor app that runs on WebAssembly &amp; a Windows, Linux, macOS app from a Blazor app that run on Electron and .NET Core. This template can be used for web apps with rich dynamic user interfaces (UIs).</description>
	<releaseNotes>Added some changes on the ElectronNET template in order to run it on WebAssembly engine if requested</releaseNotes>
	<copyright>Copyright 2020</copyright>
	<tags>aspnet templates blazor spa web webassembly xamarin android ios uwp electron windows linux macos</tags>
    <packageTypes>
      <packageType name="Template" />
    </packageTypes>
  </metadata>
  <files>
    <file
      src="content/**"
      exclude="**/bin/**;**/obj/**;**/.vs/**;**/*.csproj.user"
      target="Content" />
  </files>
</package>
